{
  "name": "ruby-refactoring",
  "displayName": "ruby-refactoring",
  "description": "A port of vim-ruby-refactoring (https://github.com/ecomba/vim-ruby-refactoring) for VSCode",
  "version": "0.0.1",
  "license": "MIT",
  "engines": {
    "vscode": "^1.77.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "ruby-refactoring.rspecExtractLet",
        "title": "RSpec Extract Let"
      },
      {
        "command": "ruby-refactoring.convertHashKeys",
        "title": "Convert Hash Keys"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "yarn run compile && yarn run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@types/sinon": "^10.0.14",
    "@types/vscode": "^1.77.0",
    "@typescript-eslint/eslint-plugin": "^5.59.1",
    "@typescript-eslint/parser": "^5.59.1",
    "@vscode/test-electron": "^2.3.0",
    "eslint": "^8.39.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "sinon": "^15.0.4",
    "typescript": "^5.0.4"
  }
}
